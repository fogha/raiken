{
  "name": "@raiken/cli",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "CLI tool for Raiken - AI-powered Playwright test generator",
  "main": "dist/index.js",
  "bin": {
    "raiken": "bin/raiken.js"
  },
  "files": [
    "bin/",
    "dist/",
    "templates/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "changelog": "auto-changelog --package --template keepachangelog"

  },
  "keywords": [
    "playwright",
    "testing",
    "ai",
    "e2e",
    "automation",
    "cli"
  ],
  "author": "Raiken Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "express": "^4.18.2",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "open": "^8.4.2",
    "detect-port": "^1.5.1",
    "glob": "^10.3.10",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.10",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/raiken.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/yourusername/raiken/issues"
  },
  "homepage": "https://raiken.dev"
} 